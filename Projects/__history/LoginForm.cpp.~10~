//---------------------------------------------------------------------------

#include <fmx.h>
#pragma hdrstop

#include "LoginForm.h"
#include "HomePage.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.fmx"
TForm3 *Form3;
//---------------------------------------------------------------------------
__fastcall TForm3::TForm3(TComponent* Owner)
	: TForm(Owner)
{
  TForm1 *HomePage = new TForm1(this);

  try
  {
       HomePage->ShowModal();
  }

  __finally
  {
      delete HomePage;
  }
}
//---------------------------------------------------------------------------

